{
  "groups": [
    {
      "name": "copilot.filter",
      "type": "com.awesomecopilot.boot.web.autoconfig.CopilotFilterProperties",
      "sourceType": "com.awesomecopilot.boot.web.autoconfig.CopilotFilterProperties"
    },
    {
      "name": "copilot.locale",
      "type": "com.awesomecopilot.boot.web.autoconfig.LocaleProperties",

    },
    {
      "name": "copilot.mvc",
      "type": "com.awesomecopilot.boot.web.autoconfig.CopilotMvcProperties",
      "sourceType": "com.awesomecopilot.boot.web.autoconfig.CopilotMvcProperties"
    },
    {
      "name": "copilot.websocket",
      "type": "com.awesomecopilot.boot.web.autoconfig.CopilotWebsocketProperties",
      "sourceType": "com.awesomecopilot.boot.web.autoconfig.CopilotWebsocketProperties"
    }
  ],
  "properties": [
    {
      "name": "copilot.filter.repeated-read",
      "type": "java.lang.Boolean",
      "description": "是否注册HttpServletRequestRepeatedReadFilter, 以开启重复读取RequestBody功能",
      "sourceType": "com.awesomecopilot.boot.web.autoconfig.CopilotFilterProperties",
      "defaultValue": false
    },
    {
      "name": "copilot.locale.cookie-name",
      "type": "java.lang.String",
      "description": "通过Cookie切换locale时, 设置的Cookie的名字",
      "sourceType": "com.awesomecopilot.boot.web.autoconfig.LocaleProperties",
      "defaultValue": "lang"
    },
    {
      "name": "copilot.locale.enabled",
      "type": "java.lang.Boolean",
      "description": "是否启用动态切换locale",
      "sourceType": "com.awesomecopilot.boot.web.autoconfig.LocaleProperties",
      "defaultValue": false
    },
    {
      "name": "copilot.locale.ignore-invalid-locale",
      "type": "java.lang.Boolean",
      "description": "ignoreInvalidLocale: lang=zh_CN这个参数值如果传了不合法的locale字符串, 是否忽略, 默认true",
      "sourceType": "com.awesomecopilot.boot.web.autoconfig.LocaleProperties",
      "defaultValue": true
    },
    {
      "name": "copilot.locale.param-name",
      "type": "java.lang.String",
      "description": "默认lang, url里面可以通过参数lang=zh_CN来切换",
      "sourceType": "com.awesomecopilot.boot.web.autoconfig.LocaleProperties",
      "defaultValue": "lang"
    },
    {
      "name": "copilot.mvc.idemtotent-token-ttl",
      "type": "java.lang.Integer",
      "description": "保障接口幂等性的token有效期, 单位秒, 默认1小时",
      "sourceType": "com.awesomecopilot.boot.web.autoconfig.CopilotMvcProperties"
    },
    {
      "name": "copilot.mvc.rest-exception-advice-enabled",
      "type": "java.lang.Boolean",
      "sourceType": "com.awesomecopilot.boot.web.autoconfig.CopilotMvcProperties",
      "defaultValue": true
    },
    {
      "name": "copilot.websocket.enabled",
      "type": "java.lang.Boolean",
      "description": "是否启用WebSocket支持",
      "sourceType": "com.awesomecopilot.boot.web.autoconfig.CopilotWebsocketProperties",
      "defaultValue": false
    },
    {
      "name": "copilot.websocket.path-prefix",
      "type": "java.lang.String",
      "description": "Websocket 过滤器匹配的URI前缀",
      "sourceType": "com.awesomecopilot.boot.web.autoconfig.CopilotWebsocketProperties",
      "defaultValue": "\/ws\/push\/**"
    }
  ],
  "hints": []
